{
  "skill_templates": [
    {
      "id": "template_web_research",
      "name": "Web Research",
      "description": "Search the web for a topic and compile findings into a structured document",
      "category": "information_gathering",
      "difficulty": "beginner",
      "estimated_time": "5 minutes",
      "tools_used": ["web_search", "file_write"],
      "workflow": {
        "nodes": [
          {
            "id": "node_1",
            "type": "tool",
            "position": {"x": 100, "y": 100},
            "data": {
              "tool_name": "web_search",
              "parameters": {"query": "${input.query}"}
            }
          },
          {
            "id": "node_2",
            "type": "tool",
            "position": {"x": 100, "y": 250},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/tmp/research.md",
                "content": "${node_1.results}"
              }
            }
          }
        ],
        "edges": [
          {
            "id": "edge_1",
            "source": "node_1",
            "target": "node_2",
            "type": "data"
          }
        ]
      },
      "use_cases": [
        "Research quantum computing",
        "Find information about AI safety",
        "Look up latest trends in web development"
      ]
    },
    {
      "id": "template_data_analysis",
      "name": "Data Analysis Pipeline",
      "description": "Read data file, perform calculations, and create visualization",
      "category": "data_processing",
      "difficulty": "intermediate",
      "estimated_time": "10 minutes",
      "tools_used": ["file_read", "calculator", "image_gen"],
      "workflow": {
        "nodes": [
          {
            "id": "node_1",
            "type": "tool",
            "position": {"x": 100, "y": 100},
            "data": {
              "tool_name": "file_read",
              "parameters": {"path": "${input.data_file}"}
            }
          },
          {
            "id": "node_2",
            "type": "tool",
            "position": {"x": 100, "y": 250},
            "data": {
              "tool_name": "calculator",
              "parameters": {"expression": "${input.analysis_formula}"}
            }
          },
          {
            "id": "node_3",
            "type": "tool",
            "position": {"x": 100, "y": 400},
            "data": {
              "tool_name": "image_gen",
              "parameters": {
                "prompt": "Chart showing ${node_2.result}",
                "size": "512x512"
              }
            }
          }
        ],
        "edges": [
          {
            "id": "edge_1",
            "source": "node_1",
            "target": "node_2",
            "type": "data"
          },
          {
            "id": "edge_2",
            "source": "node_2",
            "target": "node_3",
            "type": "data"
          }
        ]
      },
      "use_cases": [
        "Analyze sales data and create chart",
        "Process survey results",
        "Calculate financial metrics"
      ]
    },
    {
      "id": "template_content_creator",
      "name": "Content Creator",
      "description": "Research topic, generate draft, and polish content",
      "category": "content_creation",
      "difficulty": "intermediate",
      "estimated_time": "8 minutes",
      "tools_used": ["web_search", "file_write", "file_edit"],
      "workflow": {
        "nodes": [
          {
            "id": "node_1",
            "type": "tool",
            "position": {"x": 100, "y": 100},
            "data": {
              "tool_name": "web_search",
              "parameters": {"query": "${input.topic}"}
            }
          },
          {
            "id": "node_2",
            "type": "tool",
            "position": {"x": 100, "y": 250},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/tmp/draft.md",
                "content": "${node_1.results}"
              }
            }
          },
          {
            "id": "node_3",
            "type": "tool",
            "position": {"x": 100, "y": 400},
            "data": {
              "tool_name": "file_edit",
              "parameters": {
                "path": "/tmp/draft.md",
                "changes": "Format as ${input.format} with professional tone"
              }
            }
          }
        ],
        "edges": [
          {
            "id": "edge_1",
            "source": "node_1",
            "target": "node_2",
            "type": "data"
          },
          {
            "id": "edge_2",
            "source": "node_2",
            "target": "node_3",
            "type": "data"
          }
        ]
      },
      "use_cases": [
        "Create blog post about AI trends",
        "Write article about climate change",
        "Generate product description"
      ]
    },
    {
      "id": "template_code_helper",
      "name": "Code Helper",
      "description": "Search for code examples, test them, and save verified code",
      "category": "development",
      "difficulty": "advanced",
      "estimated_time": "12 minutes",
      "tools_used": ["web_search", "code_exec", "file_write"],
      "workflow": {
        "nodes": [
          {
            "id": "node_1",
            "type": "tool",
            "position": {"x": 100, "y": 100},
            "data": {
              "tool_name": "web_search",
              "parameters": {"query": "python ${input.topic} example"}
            }
          },
          {
            "id": "node_2",
            "type": "transform",
            "position": {"x": 100, "y": 250},
            "data": {
              "operation": "extract_code_blocks",
              "config": {"language": "python"}
            }
          },
          {
            "id": "node_3",
            "type": "tool",
            "position": {"x": 100, "y": 400},
            "data": {
              "tool_name": "code_exec",
              "parameters": {"code": "${node_2.code}"}
            }
          },
          {
            "id": "node_4",
            "type": "decision",
            "position": {"x": 100, "y": 550},
            "data": {
              "condition": {"expression": "${node_3.success} === true"},
              "branches": {"true": "node_5", "false": "node_6"}
            }
          },
          {
            "id": "node_5",
            "type": "tool",
            "position": {"x": 50, "y": 700},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/tmp/verified_example.py",
                "content": "${node_2.code}"
              }
            }
          },
          {
            "id": "node_6",
            "type": "tool",
            "position": {"x": 200, "y": 700},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/tmp/error_log.txt",
                "content": "Code execution failed: ${node_3.error}"
              }
            }
          }
        ],
        "edges": [
          {
            "id": "edge_1",
            "source": "node_1",
            "target": "node_2",
            "type": "data"
          },
          {
            "id": "edge_2",
            "source": "node_2",
            "target": "node_3",
            "type": "data"
          },
          {
            "id": "edge_3",
            "source": "node_3",
            "target": "node_4",
            "type": "data"
          },
          {
            "id": "edge_4",
            "source": "node_4",
            "target": "node_5",
            "type": "conditional",
            "data": {"branch": "true"}
          },
          {
            "id": "edge_5",
            "source": "node_4",
            "target": "node_6",
            "type": "conditional",
            "data": {"branch": "false"}
          }
        ]
      },
      "use_cases": [
        "Find sorting algorithm example",
        "Get REST API client code",
        "Search for database query patterns"
      ]
    },
    {
      "id": "template_file_organizer",
      "name": "Smart File Organizer",
      "description": "Read files, analyze content, and organize by type",
      "category": "automation",
      "difficulty": "advanced",
      "estimated_time": "15 minutes",
      "tools_used": ["file_read", "file_write"],
      "workflow": {
        "nodes": [
          {
            "id": "node_1",
            "type": "loop",
            "position": {"x": 100, "y": 100},
            "data": {
              "iteration_source": "${input.file_list}",
              "loop_variable": "file",
              "body_nodes": ["node_2", "node_3", "node_4"],
              "max_iterations": 100
            }
          },
          {
            "id": "node_2",
            "type": "tool",
            "position": {"x": 100, "y": 250},
            "data": {
              "tool_name": "file_read",
              "parameters": {"path": "${file}"}
            }
          },
          {
            "id": "node_3",
            "type": "transform",
            "position": {"x": 100, "y": 400},
            "data": {
              "operation": "detect_content_type",
              "config": {"content": "${node_2.content}"}
            }
          },
          {
            "id": "node_4",
            "type": "decision",
            "position": {"x": 100, "y": 550},
            "data": {
              "condition": {"expression": "${node_3.type}"},
              "branches": {
                "code": "node_5",
                "text": "node_6",
                "default": "node_7"
              }
            }
          },
          {
            "id": "node_5",
            "type": "tool",
            "position": {"x": 50, "y": 700},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/organized/code/${file}",
                "content": "${node_2.content}"
              }
            }
          },
          {
            "id": "node_6",
            "type": "tool",
            "position": {"x": 150, "y": 700},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/organized/docs/${file}",
                "content": "${node_2.content}"
              }
            }
          },
          {
            "id": "node_7",
            "type": "tool",
            "position": {"x": 250, "y": 700},
            "data": {
              "tool_name": "file_write",
              "parameters": {
                "path": "/organized/misc/${file}",
                "content": "${node_2.content}"
              }
            }
          }
        ],
        "edges": []
      },
      "use_cases": [
        "Organize downloads folder",
        "Sort project files by type",
        "Clean up desktop files"
      ]
    }
  ]
}
